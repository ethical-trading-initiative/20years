{% import "macros/timeline.njk" as timeline %}

{% extends "html.njk" %}

{% block content %}

<header class="page-header">
  <h1>Ethical Trading Initiative - 20 years</h1>
</header>

<div class="timeline">

  {% for entry in entries %}
  {{ timeline.entry(
    entry[0],
    entry[1],
    entry[2],
    entry[3],
    entry[4]
    )
  }}
  {% endfor %}

</div>

{% endblock %}
