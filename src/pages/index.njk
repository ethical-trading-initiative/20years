{% import "macros/timeline.njk" as timeline %}

{% extends "html.njk" %}

{% block content %}

<header class="page-header">
  <h1>Ethical Trading Initiative - 20 years</h1>
</header>

<div class="timeline">

  {% for entry in entries %}
  {{ timeline.entry(
    entry.type,
    entry.date,
    entry.title,
    entry.body,
    entry.btn_url
    )
  }}
  {% endfor %}

</div>

{% endblock %}
